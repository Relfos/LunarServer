<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <FileVersion>1.2.1.0</FileVersion>
    <Version>1.2.1</Version>
    <Description>HTTP server with minimal dependencies</Description>
    <Copyright>Sergio Flores</Copyright>
    <PackageProjectUrl>https://github.com/Relfos/LunarServer</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Relfos/LunarServer/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Relfos/LunarServer</RepositoryUrl>
    <PackageTags>http server cookies oauth</PackageTags>
    <Authors>Sergio Flores</Authors>
    <Company>Lunar Labs</Company>
    <Product>LunarLabs.Server</Product>
    <AssemblyName>LunarLabs.Server</AssemblyName>
    <RootNamespace>LunarLabs.WebServer</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LunarParser" Version="1.1.2" />
  </ItemGroup>

</Project>
